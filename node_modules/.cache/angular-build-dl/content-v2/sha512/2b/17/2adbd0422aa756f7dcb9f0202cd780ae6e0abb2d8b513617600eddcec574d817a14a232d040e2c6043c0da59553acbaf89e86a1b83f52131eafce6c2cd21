let myfirstcomponent = class myfirstcomponent {
    constructor(httpClient, nav, router) {
        this.httpClient = httpClient;
        this.nav = nav;
        this.router = router;
        this.messages = [
            { 'id': 1 },
            { 'id': 2 },
        ];
        this.categories = [];
        this.httpOptions = {
            headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_3__["HttpHeaders"]({ 'content-Type': 'application/json' })
        };
        this.titles = [];
    }
    // 返回按钮
    canGoBack() {
        this.nav.back();
    }
    goDetail(title) {
        console.log(title);
        this.router.navigate(['/detail'], {
            queryParams: {
                title: title
            }
        });
        //  this.router.navigateByUrl("/detail",{queryParams:{'name':'yxman'}})
        //  return true;
    }
    ngOnInit() {
        this.httpClient.
            get("http://39.99.159.233:8080/bk/categories", this.httpOptions).subscribe(res => {
            this.categories = JSON.parse(res.toString()).data;
            console.log(this.categories);
        });
    }
    doOnClick(key) {
        this.httpClient.
            get("http://39.99.159.233:8080/bk/categories?categorykey=" + key, this.httpOptions).subscribe(res => {
            this.titles = JSON.parse(res.toString()).data.pages;
            console.log(this.titles);
        });
    }
};
